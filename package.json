{
  "name": "shift-rota",
  "version": "0.1.1",
  "engines": {
    "node": ">=8.9.1"
  },
  "private": true,
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "dotenv": "^4.0.0",
    "express": "4.13.3",
    "jsonwebtoken": "^7.4.2",
    "mongodb": "^2.2.31",
    "mongoose": "^5.2.15",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pdfreader": "^0.2.4",
    "supervisor": "^0.12.0"
  },
  "scripts": {
    "start": "node server",
    "heroku-postbuild": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cross-env NODE_ENV=development supervisor -w server server/index.js",
    "client": "node start-client.js",
    "clear-uploads": "find ./uploads/pdf ! -name '*.pdf' -type f -delete"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "cross-env": "^5.1.5",
    "nodemon": "^1.11.0"
  }
}
